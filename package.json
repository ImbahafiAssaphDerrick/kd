{
  "name": "notes-app",
  "version": "1.0.0",
  "private": true,
  "description": "Library / notes app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "test:unit": "jest --testPathPattern=__tests__/unit --runInBand",
    "test:integration": "jest --testPathPattern=__tests__/integration --runInBand",
    "test:e2e": "jest --testPathPattern=__tests__/e2e --runInBand",
    "test": "npm run test:unit",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "devops",
    "notes"
  ],
  "author": "DevOps Student",
  "license": "MIT"
}
